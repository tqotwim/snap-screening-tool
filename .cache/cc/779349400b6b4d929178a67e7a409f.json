n\r\n.split.left .split-button:hover, button.threeColumns:hover, button:hover {\r\n    background-color: var(--left-button-hover-color);\r\n    border-color: var(--left-button-hover-color);\r\n}\r\n\r\n.split.right .split-button:hover, button.continue:hover {\r\n    background-color: var(--right-button-hover-color);\r\n    border-color: var(--right-button-hover-color);\r\n}\r\n\r\n.container {\r\n    width: 100vw;\r\n    height: 100vh;\r\n    background: var(--container-bg-color);\r\n}\r\n.split-container {\r\n  position: relative;\r\n  \r\n}\r\n\r\n.split {\r\n    position: absolute;\r\n    width: 50%;\r\n    height: 100%;\r\n    overflow: hidden;\r\n}\r\n\r\n.split.left{\r\n    left: 0;\r\n}\r\n.snap {\r\n  background: url(\"./img/foodlips1.jpg\") center center no-repeat;\r\n  background-size: cover;\r\n}\r\n\r\n\r\n.split.left:before {\r\n    position: absolute;\r\n    content: \"\";\r\n    width: 100vw;\r\n    height: 100vh;\r\n    background: var(--left-bg-color);\r\n\r\n}\r\n.split.right {\r\n    right: 0;\r\n}\r\n.no-snap {\r\n  background: url(\"./img/nosnap1.jpg\") center center no-repeat;\r\n  background-size: cover;\r\n}\r\n\r\n.split.right:before {\r\n    position: absolute;\r\n    content: \"\";\r\n    width: 100vw;\r\n    height: 100vh;\r\n    background: var(--right-bg-color);\r\n\r\n}\r\n.split.left, .split.right, .split.right:before, split.left:before {\r\n    transition: var(--speed) all ease-in-out;\r\n}\r\n.hover-left .left {\r\n    width: var(--hover-width);\r\n}\r\n\r\n.hover-left .right {\r\n    width: var(--other-width);\r\n}\r\n.hover-left .right:before {\r\n    z-index: 2;\r\n\r\n}\r\n.hover-right .right {\r\n    width: var(--hover-width);\r\n}\r\n\r\n.hover-right .left {\r\n    width: var(--other-width);\r\n}\r\n.hover-right .left:before {\r\n    z-index: 2;\r\n}\r\n\r\n\r\n.three-columns {\r\n    background: linear-gradient(90deg, rgba(223,39,39, .6) 33.33%, rgba(95,95,95, .6) 33.33%, rgba(95,95,95, .7) 66.66%, rgba(223,39,39, .7) 66.66%),\r\n                url(\"./img/foodlips1.jpg\")center center no-repeat;\r\n/*     background-size: cover; */\r\n\r\n}\r\n\r\n  #four-days {\r\n    display: flex;\r\n    flex-flow: column;\r\n    justify-content: center;\r\n    align-items: center;\r\n    background: \r\n    linear-gradient( rgba(223,39,39, .6) 0%, rgba(223,39,39, .6) 50%, rgba(121, 67, 67, 0.7) 50%,rgba(109, 88, 88, 0.8) 100%),\r\n    /* bottom, image */\r\n    url(./img/four-upside-down.jpg) fixed top no-repeat;\r\n  }\r\n#four-days-else h2 {\r\n    top: 20%;\r\n}\r\n.three-columns, #else-expedited-page, #else-not-expedited-page, #no-appt-page {\r\n  display: flex;\r\n\r\n}\r\n.three-columns button {\r\n  margin: .5rem;\r\n}\r\n\r\n.row {\r\n  display: flex;\r\n  flex-direction: row;\r\n  justify-content: space-around;\r\n  align-items: center;\r\n  flex-grow: 1;\r\n}\r\n\r\n.column {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-around;\r\n  align-items: center;\r\n  flex-grow: 1;\r\n}\r\n\r\n#no-appt-page, #else-expedited-page, #else-not-expedited-page {\r\n    display: flex;\r\n    align-items: center;\r\n    flex-direction: column;\r\n    justify-content: space-around;\r\n    \r\n}\r\n#else-not-expedited-page {\r\n    background-color: rgba(95, 6, 6, 0.9);\r\n}\r\n#no-appt-page {\r\n  background: \r\n      linear-gradient(\r\n        var(--right-bg-color), \r\n        var(--right-bg-color)\r\n      ),\r\n      /* bottom, image */\r\n      url(\"./img/congrats.jpg\") top center no-repeat;\r\n      background-size: cover;\r\n}\r\n\r\n@media only screen and (max-width: 755px) {\r\n  h1{\r\n\r\n    top: 0;\r\n    font-size: 3rem;\r\n  }\r\n  h2 {\r\n    font-size: 2rem;\r\n  }\r\n  h3 {\r\n    font-size: 1.5rem;\r\n  }\r\n  button {\r\n    width: 7rem;\r\n    height: 2.5rem;\r\n    font-size: .8rem;\r\n    }\r\n  .three-column button\r\n  {\r\n    width: 4rem;\r\n  }\r\n  p {\r\n    font-size: 1.3rem;\r\n    }\r\n}\r\n\r\n@media only screen and (max-height: 700px){\